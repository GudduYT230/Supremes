<script>
function show(id){
  document.querySelectorAll("section").forEach(s=>s.classList.remove("active"));
  document.getElementById(id).classList.add("active");
  if(id==='top'){document.getElementById("topSound").play();}
}
function login(){
  const p=document.getElementById("pass").value;
  msg.innerText=p==="SUPREMES"?(show("welcome"),""):"WRONG PASSWORD";
}
function toggle(card){const d=card.querySelector(".details");d.style.display=d.style.display==="block"?"none":"block";}
function music(){const m=document.getElementById("bgm");m.paused?m.play():m.pause();}

const particlesContainer=document.getElementById('particles');
for(let i=0;i<60;i++){
  const p=document.createElement('div');p.classList.add('particle');
  p.style.left=Math.random()*100+'%';
  p.style.width=p.style.height=(Math.random()*4+2)+'px';
  p.style.animationDuration=(Math.random()*5+3)+'s';
  particlesContainer.appendChild(p);
}
const streaksContainer=document.getElementById('streaks');
for(let i=0;i<30;i++){
  const s=document.createElement('div');s.classList.add('streak');
  s.style.left=Math.random()*100+'%';
  s.style.animationDuration=(Math.random()*5+2)+'s';
  streaksContainer.appendChild(s);
}

let discordCount = 6;
function updateDiscordCount(){
  let f=Math.floor(Math.random()*3);
  document.getElementById("discordCount").innerText = discordCount + f;
}
setInterval(updateDiscordCount,2000);
updateDiscordCount();
</script>

